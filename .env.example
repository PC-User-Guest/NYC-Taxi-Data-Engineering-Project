# Example .env values (DO NOT commit real secrets)
# PostgreSQL
POSTGRES_USER=nyc_taxi_user
POSTGRES_PASSWORD=change_me
POSTGRES_DB=nyc_taxi
PG_HOST=localhost
PG_PORT=15432

# pgAdmin (use a different strong password for pgAdmin)
PGADMIN_DEFAULT_EMAIL=admin@example.com
PGADMIN_DEFAULT_PASSWORD=change_me

# Terraform GCP project SSH_cidr and Admin_cidr
TF_VAR_ssh_cidr=["your_ip_address/32"]
TF_VAR_admin_cidr=["your_ip_address/32"]

# Optional: override dataset sources
#GREEN_PARQUET_URL=https://...
#ZONES_CSV_URL=https://...

# Ingestion tuning
# INGEST_CHUNK_SIZE=10000
